<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">



    <dubbo:reference id="registryService" interface="com.alibaba.dubbo.registry.RegistryService" check="false" />

    <bean class="com.dubboclub.admin.sync.RegistryServerSync" id="registryServerSync">
        <property name="registryService" ref="registryService"/>
    </bean>

    <bean class="com.dubboclub.admin.service.AbstractService" id="abstractService" abstract="true">
        <property name="registryServerSync" ref="registryServerSync"/>
    </bean>

    <bean class="com.dubboclub.admin.service.impl.ProviderServiceImpl" parent="abstractService"/>
    <bean class="com.dubboclub.admin.service.impl.ApplicationServiceImpl" parent="abstractService"/>
</beans>